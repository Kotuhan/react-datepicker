.historyView {
  width: 248px;
  height: 242px;
  position: relative;
  border-radius: 3px;
  margin-top: 40px;
  background-color: #ffffff;
  box-shadow: -0.1px 2px 6px 0 rgba(0, 0, 0, 0.2);
  border: solid 1px #cccccc;
  padding: 9px;

  .verticalArrow {
      position: absolute;
      left: 50%;
  }
}
